# vim: set syntax=python:

def build(bld):
    t = bld.program(
        name = 'inttag.e',
        source = 'inttag.c',
        target = 'inttag.e',
        use = ['calstis','tables'] + bld.env.LOCAL_LIBS,
        lib = bld.env.EXTERNAL_LIBS,
        libpath = bld.env.LIBPATH,
        install_path = '${PREFIX}/bin'
        )